<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.BookHub.Storage.StorageRepository">

    <insert id="insertStorage" useGeneratedKeys="true" keyProperty="id" parameterType="storagedto">
        INSERT INTO storage (document_id, url, save_at)
        values (#{document_id}, #{url}, now())
    </insert>
</mapper>
